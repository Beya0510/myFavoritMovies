@using Favies.Services
@inject GetMoviesService GetMoviesService

<h3>Data from API</h3>

@if (data == null)
{
    <p>Loading...</p>
}
else
{
    <ul>
        @foreach (var item in data)
        {
            <li>@item</li>
        }
    </ul>
}

@code {
    private List<string>? data; // DÃ©clarer comme nullable

    protected override async Task OnInitializedAsync()
    {
        data = await GetMoviesService.GetMoviesAsync<List<string>>("https://www.omdbapi.com/?apikey=YOUR_API_KEY");
    }
}